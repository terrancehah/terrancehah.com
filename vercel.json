{
    "cleanUrls": true,
    "functions": {
        "api/**/*.py": {
            "runtime": "python3.9"
        }
    },
    "rewrites": [
        {
            "source": "/projects/fastapi-experiment",
            "destination": "/api"
        },
        {
            "source": "/projects/fastapi-experiment/:path*",
            "destination": "/api"
        }
    ]
}