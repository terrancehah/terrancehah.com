{
    "cleanUrls": true,
    "version": 2,
    "builds": [
        {
        "src": "travel-rizz-server.js", 
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/$1",
            "status": 200,
            "headers": {
                "Content-Type": "application/json",
                "Cache-Control": "s-maxage=43200"
            }
        }
    ]
}
